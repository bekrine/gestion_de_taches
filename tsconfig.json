{
    "compilerOptions": {
      "esModuleInterop": true,
      "module": "commonjs",
      "target": "es5",
      "lib": ["es6", "dom"],
      "sourceMap": true,
      "allowJs": true,
      "jsx": "react",
      "moduleResolution": "node",
      "rootDir": "src",
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noEmit": true,
      "allowImportingTsExtensions":true,
     
    },
    "include": ["src/**/*.tsx"],
    "outDir": "./dist",
    "exclude": [
      "node_modules",
      "build",
    ],
    "types": [
      "typePatches"
    ]
  }